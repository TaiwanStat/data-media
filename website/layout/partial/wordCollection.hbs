<div class="card-container">
	<div class="list" v-bind:class="list.class" v-for="list in lists">
		<h3>${list.name}</h3>
		<a target="_blank" class="card" v-bind:link="card.link" v-for="card in list.cards">
			<h5>${card.header}</h5>
			<div class="detail">${card.detail}</div>
    	</a>
	</div>
</div>